{
  "name": "angular-template",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && webpack-dev-server --config webpack.config.js --inline --port 4200 --colors --watch --content-base src",
    "bundle": "webpack --config webpack.config.js --source-map",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings install",
    "internationalize": "ng-xi18n -p tsconfig.json"
  },
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "systemjs": "0.19.27",
    "es6-shim": "0.35.3",
    "reflect-metadata": "^0.1.3",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.0",
    "@angular/platform-server": "^2.4.0",
    "@types/core-js": "^0.9.35",
    "@types/node": "~6.0.60",
    "typescript": "~2.0.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "ts-loader": "1.3.3",
    "html-loader": "0.4.4",
    "css-loader": "0.26.1",
    "text-loader": "^0.0.1"
  },
  "license": "MIT",
  "author": {
    "name": "Sahil Malik",
    "url": "www.winsmarts.com"
  },
  "keywords": [
    "angular",
    "webpack",
    "angular2",
    "typescript"
  ],
  "description": "This project uses Angular with WebPack to craft up a starter angular project."
}